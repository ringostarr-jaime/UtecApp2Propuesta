<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorAccent</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="windowActionBar">false</item>


    </style>



    <style name="SCBSwitch" parent="Theme.AppCompat.Light">
        <item name="colorControlActivated">@color/colorPrimary</item>
        <item name="colorControlHighlight">@color/colorPrimary</item>
        <!-- active thumb & track color (30% transparency) -->


        <!-- inactive thumb color -->
        <item name="colorSwitchThumbNormal">#f1f1f1 </item>

        <!-- inactive track color (30% transparency) -->
        <item name="android:colorForeground">#42221f1f</item>
    </style>



    <style name="CampoTextovino" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="colorControlNormal">@color/colorPrimaryDark</item>
        <item name="colorControlActivated">@color/colorPrimaryDark</item>
        <item name="android:textColorHighlight">@color/colorPrimaryDark</item>
    </style>

    <style name="main" parent="Theme.AppCompat">
        <item name="colorPrimary">@color/colonegro</item>
        <item name="colorPrimaryDark">@color/colonegro</item>
        <item name="colorAccent">@color/colonegro</item>
    </style>


    <style name="Temablanco" parent="Theme.AppCompat">
        <item name="colorPrimary">@color/blanco</item>
        <item name="colorPrimaryDark">@color/blanco</item>
        <item name="colorAccent">@color/blanco</item>



    </style>

    <style name="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />

</resources>
